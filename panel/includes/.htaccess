RewriteRule ^/?inicio/(.+)$                                                   index.php?envio=$1 [L]

RewriteRule ^/?noticias/([0-9]+)/(.*)$                                        news.php?id=$1 [L]

RewriteRule ^/?estudio/([0-9]+)$	                                             plusurbano.php?envio=$1 [L]
RewriteRule ^estudio/?$        		                                          plusurbano.php [L]

# RewriteRule ^/?proyectos/([0-9]+)$	                                          proyectos.php?envio=$1
# RewriteRule ^proyectos/?$        		                                          proyectos.php

# RewriteRule ^/?planificacion-regional-y-ecologica/([0-9]+)$		               desarrollos.php?cat=1&envio=$1
# RewriteRule ^planificacion-regional-y-ecologica/?$				                  desarrollos.php?cat=1

# RewriteRule ^/?diseno-urbano/([0-9]+)$							                     desarrollos.php?cat=2&envio=$1
# RewriteRule ^diseno-urbano/?$									                        desarrollos.php?cat=2

# RewriteRule ^/?paisaje-y-arquitectura/([0-9]+)$					                  desarrollos.php?cat=3&envio=$1
# RewriteRule ^paisaje-y-arquitectura/?$							                     desarrollos.php?cat=3

RewriteRule ^innovacion-e-investigacion/?$						                  innova.php  [L]
RewriteRule ^/?innovacion-e-investigacion/([0-9]+)$				               innova.php?envio=$1  [L]

RewriteRule ^/?innovacion-e-investigacion/([0-9]+)/(.*)/([0-9]+)$		         innova.php?id=$1&envio=$2  [L]

RewriteRule ^/?proyectos/?$	                                                desarrollos.php  [L]
RewriteRule ^/?proyectos/([0-9]+)/?$										            desarrollos.php&envio=$1  [L]
RewriteRule ^/?proyecto/([0-9]+)/(.+)/([0-9]+)$		                           trabajos.php?id=$1&name=$2&envio=$4  [L]

RewriteRule ^academia-de-arquitectura-del-paisaje/?$				               academia.php [L]
RewriteRule ^academia-de-arquitectura-del-paisaje/([0-9]+)/(.+)?$		         academia.php?id=$1 [L]
RewriteRule ^academia-de-arquitectura-del-paisaje/([0-9]+)/(.+)/([0-9]+)?$		academia.php?id=$1&envio=$3 [L]

RewriteRule ^/?tienda/([0-9]+)$				                                    tienda.php?envio=$1 [L]
RewriteRule ^/?tienda/?$										                        tienda.php [L]

